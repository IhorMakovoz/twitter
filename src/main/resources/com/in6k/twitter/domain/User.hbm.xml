<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.in6k.twitter.domain.User" table="users">
        <id column="id" name="id" type="java.lang.Integer">
            <generator class="native"/>
        </id>

        <property column="login" name="login" type="java.lang.String"/>
        <property column="password" name="password" type="java.lang.String"/>

   <!--<many-to-one name="department" column="department_id" class="com.in6k.hibernate.domain.Department"/>-->
    </class>
</hibernate-mapping>